
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Admin/Index.cshtml";
}

<h2 class="mb-4">İstatistikler</h2>

<div class="alert alert-success">

    Toplam Mesaj Sayısı:<b> @ViewBag.totalmessageCount</b>

</div>
<div class="alert alert-warning">


    Okunmuş Mesaj Sayısı:<b> @ViewBag.messageIsReadTrueCount</b>

</div>
<div class="alert alert-danger">


    Okunmamış Mesaj Sayısı:<b> @ViewBag.messageIsReadFalseCount</b>
</div>

<div class="alert alert-info">


    Yetenek Sayısı:<b> @ViewBag.skillCount</b>
</div>
<div class="alert alert-primary">


    Puan Toplam Sayısı:<b> @ViewBag.skillRateSum</b>
</div>
<div class="alert alert-secondary">


    Puan Ortalama Sayısı:<b> @ViewBag.skillRateAvg</b>
</div>

<div class="alert alert-primary" style="background-color:#ff6a00">


    En Yüksek Puanlı Yetenek:<b> @ViewBag.maxRateSkillName</b>
</div>
<div class="alert alert-primary" style="background-color:#00ff90" >


    Mesaj Sayısı:<b> @ViewBag.getMessageCountBySubjectReferances</b>
</div>
<div class="alert alert-primary">

    
    Mesaj Gönderen:<b> @ViewBag.getMessageSendPeople</b>
</div>

<div class="alert alert-dark">

    
    Harf İçeren ve Okunan:<b> @ViewBag.getMessageCountByEmailContainAAndIsReadTrue</b>
</div>



